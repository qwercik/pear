module(
    import('complex.pr'),
    
    def(main, block(
        comment('To jest komentarz'),
        print('Program liczy sumę liczb zespolonych\n'),

        print('Podaj pierwszą liczbę: '),
        read('%d %d', a, b),

        print('Podaj drugą liczbę: '),
        read('%d %d', c, d),

        eval(add(complex(a, b), complex(c, d)), result),
        print(result)
    ))
)
